{
    "name": "@ackee/mateus",
    "version": "0.2.5",
    "description": "Antd components as a form fields + redux-saga flow for easier handle of redux-form form's submit",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "build:lib": "BABEL_ENV=lib babel src --out-dir lib --extensions \".ts,.tsx\" --ignore \"**/*.story.tsx,**/*.d.ts\" --source-maps inline",
        "build:es": "BABEL_ENV=es babel src --out-dir es --extensions \".ts,.tsx\" --ignore \"**/*.story.tsx,**/*.d.ts\" --source-maps inline",
        "build:js": "npm run build:lib && npm run build:es",
        "build:types": "tsc --project tsconfig.types.json --emitDeclarationOnly",
        "build": "npm run clean && npm run type-check && npm run build:js && npm run build:types",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "cross-env BABEL_ENV=development rm -rf build && build-storybook -o build",
        "lint": "tslint \"src/**/*.{ts,tsx}\"",
        "test": "BABEL_ENV=test jest",
        "clean": "rm -rf lib es",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "start": "npm run build && onchange 'src/**/*.{ts,tsx}' -- npm run build",
        "version": "changelog-it && vim CHANGELOG.md && git add CHANGELOG.md",
        "size": "package-size ./es --no-cache"
    },
    "author": "Lukáš Horák <lukas.horak@ackee.cz>",
    "contributors": [],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/AckeeCZ/mateus"
    },
    "dependencies": {
        "@types/lodash": "^4.14.134",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "react-display-name": "^0.2.4",
        "redux-form-antd": "^3.1.6"
    },
    "peerDependencies": {
        "antd": "3.x",
        "prop-types": "15.x",
        "react": "16.x",
        "redux": "4.0.x",
        "redux-form": "^7.4.2",
        "redux-saga": "1.0.x"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@storybook/addon-actions": "^4.1.18",
        "@storybook/addon-info": "^4.1.18",
        "@storybook/addon-links": "^4.1.18",
        "@storybook/addon-options": "^4.1.18",
        "@storybook/addon-storysource": "^4.1.18",
        "@storybook/react": "^4.1.18",
        "@types/enzyme": "^3.9.4",
        "@types/jest": "^23.3.14",
        "@types/node": "^10.14.10",
        "@types/react": "^16.8.22",
        "@types/react-dom": "^16.8.4",
        "@types/react-intl": "^2.3.18",
        "@types/react-redux": "^7.1.0",
        "@types/redux": "^3.6.0",
        "@types/redux-form": "^7.4.13",
        "@types/storybook__react": "^4.0.2",
        "antd": "^3.19.8",
        "babel-jest": "^23.6.0",
        "babel-plugin-custom-import-path-transform": "^1.0.3",
        "babel-plugin-transform-imports": "^1.5.1",
        "changelog-it": "github:AckeeCZ/changelog-it",
        "cross-env": "^5.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "eslint": "^5.16.0",
        "eslint-config-ackee": "^0.9.2",
        "eslint-plugin-jest": "^22.7.1",
        "jest": "^23.6.0",
        "node-sass": "^4.12.0",
        "onchange": "^5.2.0",
        "package-size": "2.3.x",
        "prettier-config-ackee": "0.x",
        "prop-types": "^15.7.2",
        "react": "16.8.x",
        "react-dom": "16.8.x",
        "react-intl": "2.9.x",
        "react-redux": "7.1.x",
        "redux": "4.0.x",
        "redux-form": "^7.4.2",
        "redux-saga": "1.0.x",
        "sass-loader": "7.1.x",
        "ts-jest": "^23.10.5",
        "tslint": "^5.18.0",
        "tslint-config-ackee": "^0.3.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.5.2",
        "utility-types": "^2.1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
